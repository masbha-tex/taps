					<field name="sizecm" attrs="{'column_invisible':[('parent.company_id', 'in',(2,3,4))]}"/>
                   
                    <field name="back_part" attrs="{'column_invisible':[('parent.company_id', 'in',(2,1,4))]}" />
                    
